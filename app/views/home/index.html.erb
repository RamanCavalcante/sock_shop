<h1>Home#index</h1>
<h3>Olá</h3>

<% if user_signed_in? %>
    <h1>Bem vindo</h1>
    <%= button_to "logout", destroy_user_session_path, method: :delete %>
<% else %>
    <h2>Você não está logado... Por favor, se cadastre. <%= link_to "Sign up", new_user_registration_path %></h2>
    <%= link_to "login", new_user_session_path %>
<% end %>


